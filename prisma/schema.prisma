generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Keys {
  id        String   @id @default(cuid())
  key       String   @unique
  redeemed  Int      @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime
}
